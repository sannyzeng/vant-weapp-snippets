  "Basic: Layout van-row": {
    "prefix": "vanr",
    "body": [
      "<van-row>",
      "\t<van-col span=\"${1:12}\">${2}</van-col>",
      "\t<van-col span=\"${3:12}\">${4}</van-col>",
      "</van-row>"
    ],
    "description": "Vant <van-row>栅格布局<van-row>"
  },
